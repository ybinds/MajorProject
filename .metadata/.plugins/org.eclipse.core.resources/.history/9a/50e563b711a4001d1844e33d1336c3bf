package com.rihs.service;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.reactive.function.client.WebClient;

import com.rihs.entity.Case;
import com.rihs.repository.CaseRepository;

public class CaseServiceImpl implements ICaseService {

	@Autowired
	private CaseRepository crepo;
	
	public Integer createCase(Long appId) {
		Case c = new Case();
		WebClient webClient = WebClient.create();
		String stateName = webClient.get()
				.uri(REST_URL, request.getSsn())
				.retrieve()
				.bodyToMono(String.class)
				.block();
		crepo.save(null);
		return null;
	}

}
