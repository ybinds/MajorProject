package com.rihs.service;

import org.springframework.beans.factory.annotation.Autowired;

import com.rihs.entity.Case;
import com.rihs.repository.CaseRepository;

public class CaseServiceImpl implements ICaseService {

	@Autowired
	private CaseRepository crepo;
	
	public Long createCase(Long appId) {
		Case c = new Case();
		c.setAppId(appId);
		Case savedCase = crepo.save(c);
		return savedCase.getCaseNumber();
	}

}
